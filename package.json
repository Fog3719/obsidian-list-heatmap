  "keywords": [
    "obsidian",
    "plugin",
    "heatmap",
    "list",
    "diary"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "^0.13.12",
    "esbuild-plugin-copy": "^1.5.0",
    "obsidian": "latest",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "d3": "^7.8.5"
  }
}
